<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tenantfinder" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TenantFinder" >
        <activity
            android:name=".LandlordLogin"
            android:exported="true" />
        <activity
            android:name=".Messages"
            android:exported="true" />
        <activity
            android:name=".MyChatApp"
            android:exported="true" />
        <activity android:name=".tenant_form_activity" />
        <activity android:name=".landlord_form_activity" />
        <activity android:name=".TenantHomeActivity" />
        <activity
            android:name=".Notifications"
            android:label="@string/notification_s"
            android:parentActivityName=".Landlord" />
        <activity
            android:name=".View_Remove"
            android:parentActivityName=".Landlord" />
        <activity
            android:name=".PlaceDetails"
            android:parentActivityName=".Landlord" />
        <activity android:name=".Landlord" />
        <activity
            android:name=".Login"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>